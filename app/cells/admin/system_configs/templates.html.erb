<script type="text/template" id="email-config-view-template">
    <h3 class="h3-underlined"><%= t 'email-addresses-setup-title' %></h3>
    <p><%= t 'email-addresses-setup-intro' %></p>
    <div class="bs-callout bs-callout-info">
        <h4><%= t 'hint' %></h4>
        <%= t 'email-addresses-setup-hint' %>    
    </div>
    <dl class="dl-horizontal">
        {{ if (typeof id != 'undefined') { }}
        <dt><%= t "config-info-email" %></dt><dd>{{= info }}</dd>
        <dt><%= t "config-email-notifications" %></dt><dd>{{= receive_notifications ? '<%= t "yes" %>' : '<%= t "no" %>' }}</dd>
        <dt class="voffset20"></dt><dd></dd>
        <dt><%= t "config-from-email" %></dt><dd>{{= from }}</dd>
        <dt><%= t "config-reply-to-email" %></dt><dd>{{= reply_to ? reply_to : '' }}</dd>
        <dt><%= t "config-cc-email" %></dt><dd>{{= cc }}</dd>
        <dt><%= t "config-test-email" %></dt><dd>{{= test_email }}</dd>
        
        {{ } else{ }}
        NO CONFIG
        {{ } }}
    </dl>
</script>

<script type="text/template" id="cloudinary-config-view-template">
    <dl class="dl-horizontal">
        {{ if (typeof id != 'undefined') { }}
        <dt><%= t "config-cloudinary-api-key" %></dt><dd>{{= api_key }}</dd>
        <dt><%= t "config-cloudinary-api-secret" %></dt><dd>{{= api_secret }}</dd>
        <dt><%= t "config-cloudinary-cloud-name" %></dt><dd>{{= cloud_name }}</dd>
        <dt><%= t "config-cloudinary-enhance-image-tag" %></dt><dd>{{= enhance_image_tag ? 'Yes' : 'No' }}</dd>
        <dt><%= t "config-cloudinary-static-image-support" %></dt><dd>{{= static_image_support ? 'Yes' : 'No' }}</dd>
        {{ } else{ }}
        NO CONFIG
        {{ } }}
    </dl>
</script>
