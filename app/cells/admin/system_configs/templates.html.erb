<% @extra_template.each do |extra| %>
    <%= render extra %>
<% end %>

<script type="text/template" id="config-users-template">
    <%= render_cell "admin/user_list", :display, :pager => "user-pager" %>
    <%= render_cell "admin/user_list", :javascript, 
        :caption => "submenu-users",
        :pager => "#user-pager",
        :selectNotifyMethod => "UserMgmtApp.UserManagement.showDetails",
        :clearNotifyMethod => "UserMgmtApp.UserManagement.clearDetails",
        :modelType => "UserMgmtApp.UserManagement.UserMdl",
        :showControls => true
        %>
</script>

